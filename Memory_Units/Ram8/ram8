S_0x13d609670 .scope module, "ram8_tb" "ram8_tb" 2 1;
 .timescale 0 0;
v0x13d61f250_0 .var "t_address", 2 0;
v0x13d61f300_0 .var "t_clk", 0 0;
v0x13d61f390_0 .var "t_load", 0 0;
v0x13d61f460_0 .net "t_out", 15 0, v0x13d61ef90_0;  1 drivers
v0x13d61f510_0 .var "t_val", 15 0;
S_0x13d6074e0 .scope module, "my_ram8" "ram8" 2 6, 3 1 0, S_0x13d609670;
 .timescale 0 0;
    .port_info 0 /INPUT 16 "val";
    .port_info 1 /INPUT 1 "load";
    .port_info 2 /INPUT 1 "clk";
    .port_info 3 /INPUT 3 "address";
    .port_info 4 /OUTPUT 16 "out";
v0x13d6076d0_0 .net "address", 2 0, v0x13d61f250_0;  1 drivers
v0x13d61ee60_0 .net "clk", 0 0, v0x13d61f300_0;  1 drivers
v0x13d61ef00_0 .net "load", 0 0, v0x13d61f390_0;  1 drivers
v0x13d61ef90_0 .var "out", 15 0;
v0x13d61f040 .array "register_array", 0 7, 15 0;
v0x13d61f120_0 .net "val", 15 0, v0x13d61f510_0;  1 drivers
E_0x13d60f330 .event posedge, v0x13d61ee60_0;
    .scope S_0x13d6074e0;
T_0 ;
    %wait E_0x13d60f330;
    %load/vec4 v0x13d61ef00_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x13d61f120_0;
    %load/vec4 v0x13d6076d0_0;
    %pad/u 5;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x13d61f040, 0, 4;
T_0.0 ;
    %load/vec4 v0x13d6076d0_0;
    %pad/u 5;
    %ix/vec4 4;
    %load/vec4a v0x13d61f040, 4;
    %assign/vec4 v0x13d61ef90_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x13d609670;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x13d61f300_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x13d61f300_0;
    %inv;
    %store/vec4 v0x13d61f300_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x13d609670;
T_2 ;
    %vpi_call 2 12 "$monitor", "t_val=%b, t_load=%b, t_clk=%b, t_address=%b, t_out=%b", v0x13d61f510_0, v0x13d61f390_0, v0x13d61f300_0, v0x13d61f250_0, v0x13d61f460_0 {0 0 0};
    %pushi/vec4 3, 0, 16;
    %store/vec4 v0x13d61f510_0, 0, 16;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x13d61f390_0, 0, 1;
    %pushi/vec4 3, 0, 3;
    %store/vec4 v0x13d61f250_0, 0, 3;
    %delay 10, 0;
    %pushi/vec4 15, 0, 16;
    %store/vec4 v0x13d61f510_0, 0, 16;
    %pushi/vec4 5, 0, 3;
    %store/vec4 v0x13d61f250_0, 0, 3;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x13d61f390_0, 0, 1;
    %pushi/vec4 3, 0, 3;
    %store/vec4 v0x13d61f250_0, 0, 3;
    %delay 10, 0;
    %pushi/vec4 5, 0, 3;
    %store/vec4 v0x13d61f250_0, 0, 3;
    %delay 10, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_2;
