    .scope S_0x1356087e0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x13561d480_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x13561d480_0;
    %inv;
    %store/vec4 v0x13561d480_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x1356087e0;
T_2 ;
