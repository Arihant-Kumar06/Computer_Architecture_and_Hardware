    %load/vec4a v0x13ae1f200, 4;
    %assign/vec4 v0x13ae1f150_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x13ae09400;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x13ae1f4c0_0, 0, 1;
T_1.0 ;
